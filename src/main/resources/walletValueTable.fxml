<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<TableView xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
    <columns>
        <TableColumn fx:id="nameColumn" style="-fx-alignment: center-left;" text="Name" />
        <TableColumn maxWidth="140.0" minWidth="140.0" prefWidth="140.0" style="-fx-alignment: center-right;" text="Value" />
        <TableColumn maxWidth="145.0" minWidth="145.0" prefWidth="145.0" style="-fx-alignment: center-right;" text="Income Value" />
    </columns>
</TableView>
